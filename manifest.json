{
	"name": "Youtube Uncludifier",
	"version": "1.0.0",
	"manifest_version": 2,
	"description": "Removes various types of posts from youtube",
	"icons": {
		"32":  "icons/icon32.png",
		"64":  "icons/icon64.png",
		"128": "icons/icon128.png"
		},
	"permissions": [
		"activeTab", 
		"tabs",
		"storage",
		"*://www.youtube.com/feed/subscriptions"
	],
	"background": {
		"scripts": ["scripts/background.js"],
		"persistent": false
	},
	"options_page": "html/options.html"
}
